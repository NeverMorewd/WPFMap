<Application x:Class="WPFMap.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPFMap"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WPFMap;component/Resource/ChinaMapOld.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WPFMap;component/Resource/ChinaMap2.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WPFMap;component/Resource/NMWindowDictionary.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WPFMap;component/Resource/WindowTitleBarDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
