<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:WinFormHost="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
        xmlns:WinFormControls="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms">

    <ControlTemplate x:Key="ChinaMapWindowTemplate" TargetType="Window">
        <ControlTemplate.Resources>
            <Style TargetType="{x:Type Button}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border CornerRadius="20">
                                <Border.Background>
                                    <SolidColorBrush x:Name="back" Color="#40FFFFFF"></SolidColorBrush>
                                </Border.Background>
                                <TextBlock Text="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="{TemplateBinding FontSize}" Foreground="White"></TextBlock>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Trigger.EnterActions>
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <ColorAnimation From="#40FFFFFF" To="#FF8DD28A" Duration="0:0:0.3"
                                                                Storyboard.TargetName="back"
                                                                Storyboard.TargetProperty="Color"></ColorAnimation>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </Trigger.EnterActions>
                                    <Trigger.ExitActions>
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <ColorAnimation From="#FF8DD28A" To="#40FFFFFF" Duration="0:0:0.3"
                                                                Storyboard.TargetName="back"
                                                                Storyboard.TargetProperty="Color"></ColorAnimation>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </Trigger.ExitActions>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="TextBox">
                <Setter Property="HorizontalAlignment" Value="Left"></Setter>
                <Setter Property="VerticalAlignment" Value="Top"></Setter>
                <Setter Property="Margin" Value="120,30,0,0"></Setter>
                <Setter Property="Foreground" Value="White"></Setter>
                <Setter Property="FontSize" Value="20"></Setter>
                <Setter Property="CaretBrush" Value="White"></Setter>
                <Setter Property="SelectionBrush" Value="#80FFFFFF"></Setter>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="TextBox">
                            <Grid HorizontalAlignment="Stretch" Height="40" VerticalAlignment="Stretch">
                                <Border Background="#6DB5E7" CornerRadius="8" Opacity="0.8"></Border>
                                <ScrollViewer VerticalContentAlignment="Center" VerticalAlignment="Center" x:Name="PART_ContentHost" Foreground="{TemplateBinding Foreground}" FontSize="{TemplateBinding FontSize}" Margin="10,0,10,0" FontFamily="{TemplateBinding FontFamily}"></ScrollViewer>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="PasswordBox">
                <Setter Property="HorizontalAlignment" Value="Left"></Setter>
                <Setter Property="VerticalAlignment" Value="Center"></Setter>
                <Setter Property="Margin" Value="120,80,0,0"></Setter>
                <Setter Property="Foreground" Value="White"></Setter>
                <Setter Property="FontSize" Value="20"></Setter>
                <Setter Property="CaretBrush" Value="White"></Setter>
                <Setter Property="SelectionBrush" Value="#80FFFFFF"></Setter>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="PasswordBox">
                            <Grid HorizontalAlignment="Stretch" Height="40" VerticalAlignment="Stretch">
                                <Border Background="#6DB5E7" CornerRadius="8" Opacity="0.8"></Border>
                                <ScrollViewer VerticalContentAlignment="Center" VerticalAlignment="Center" x:Name="PART_ContentHost" Foreground="{TemplateBinding Foreground}" FontSize="{TemplateBinding FontSize}" Margin="10,0,10,0" FontFamily="{TemplateBinding FontFamily}"></ScrollViewer>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="TextBlock">
                <Setter Property="HorizontalAlignment" Value="Left"></Setter>
                <Setter Property="VerticalAlignment" Value="Top"></Setter>
                <Setter Property="Foreground" Value="White"></Setter>
                <Setter Property="FontSize" Value="22"></Setter>
            </Style>
            <Style x:Key="DashStrokeStyle" TargetType="Path">
                <Setter Property="Panel.ZIndex" Value="102"/>
                <Setter Property="StrokeDashArray" Value="5,1"/>
                <Setter Property="StrokeDashOffset" Value="100"/>
                <Setter Property="StrokeThickness" Value="3"/>
                <Setter Property="Stroke" Value="#f2c456"/>
            </Style>

            <Style x:Key="CircleGrid" TargetType="Control">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Control">
                            <Grid Width="75" Height="75">
                                <TextBlock Text="{TemplateBinding Name}" IsHitTestVisible="False"  Margin="16,48,0,0" FontWeight="Bold" FontFamily="YouYuan" FontSize="{TemplateBinding FontSize}" Background="{x:Null}">
                                    <TextBlock.RenderTransform>
                                        <TransformGroup>
                                            <MatrixTransform Matrix="1 0 0 -1 0 0"/>
                                        </TransformGroup>
                                    </TextBlock.RenderTransform>
                                </TextBlock>
                                <Ellipse IsHitTestVisible="False" Name="circle" Margin="25" StrokeThickness="2">
                                    <Ellipse.Stroke>
                                        <SolidColorBrush Color="Blue" x:Name="stroke"/>
                                    </Ellipse.Stroke>
                                    <Ellipse.Fill>
                                        <RadialGradientBrush   
                                                                      GradientOrigin="0.5,0.5" Center="0.5,0.5"   
                                                                      RadiusX="0.5" RadiusY="0.5">
                                            <GradientStop x:Name="inner" Color="Transparent" Offset="0.95" />
                                            <GradientStop x:Name="external" Color="Blue" Offset="1" />
                                        </RadialGradientBrush>
                                    </Ellipse.Fill>
                                    <Ellipse.Triggers>
                                        <EventTrigger RoutedEvent="ContentControl.Loaded">
                                            <BeginStoryboard>
                                                <Storyboard RepeatBehavior="Forever" >
                                                    <DoubleAnimation
                                                                            Storyboard.TargetName="inner"
                                                                            Storyboard.TargetProperty="Offset"
                                                                             From="0.95" To="0.5" Duration="0:0:1" BeginTime="0:0:0"/>
                                                    <ThicknessAnimation
                                                                            Storyboard.TargetName="circle"
                                                                            Storyboard.TargetProperty="Margin"
                                                                             From="25" To="0" Duration="0:0:2" BeginTime="0:0:0"/>
                                                    <ColorAnimation
                                                                            Storyboard.TargetName="stroke"
                                                                            Storyboard.TargetProperty="Color"
                                                                            From="Blue" To="Transparent" Duration="0:0:0.2" BeginTime="0:0:1.8"/>
                                                    <ColorAnimation
                                                                            Storyboard.TargetName="external"
                                                                            Storyboard.TargetProperty="Color"
                                                                            From="Blue" To="Transparent" Duration="0:0:0.2" BeginTime="0:0:1.8"/>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Ellipse.Triggers>
                                </Ellipse>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="CircleGrid_3" TargetType="Control">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Control">
                            <Grid Width="75" Height="75">
                                <TextBlock Text="{TemplateBinding Name}" IsHitTestVisible="False"  Margin="7,50,0,0" FontWeight="Bold" FontFamily="YouYuan" FontSize="{TemplateBinding FontSize}" Background="{x:Null}">
                                    <TextBlock.RenderTransform>
                                        <TransformGroup>
                                            <MatrixTransform Matrix="1 0 0 -1 0 0"/>
                                        </TransformGroup>
                                    </TextBlock.RenderTransform>
                                </TextBlock>
                                <Ellipse IsHitTestVisible="False" Name="circle" Margin="25" StrokeThickness="2">
                                    <Ellipse.Stroke>
                                        <SolidColorBrush Color="Blue" x:Name="stroke"/>
                                    </Ellipse.Stroke>
                                    <Ellipse.Fill>
                                        <RadialGradientBrush   
                                                                      GradientOrigin="0.5,0.5" Center="0.5,0.5"   
                                                                      RadiusX="0.5" RadiusY="0.5">
                                            <GradientStop x:Name="inner" Color="Transparent" Offset="0.95" />
                                            <GradientStop x:Name="external" Color="Blue" Offset="1" />
                                        </RadialGradientBrush>
                                    </Ellipse.Fill>
                                    <Ellipse.Triggers>
                                        <EventTrigger RoutedEvent="ContentControl.Loaded">
                                            <BeginStoryboard>
                                                <Storyboard RepeatBehavior="Forever" >
                                                    <DoubleAnimation
                                                                            Storyboard.TargetName="inner"
                                                                            Storyboard.TargetProperty="Offset"
                                                                             From="0.95" To="0.5" Duration="0:0:1" BeginTime="0:0:0"/>
                                                    <ThicknessAnimation
                                                                            Storyboard.TargetName="circle"
                                                                            Storyboard.TargetProperty="Margin"
                                                                             From="25" To="0" Duration="0:0:2" BeginTime="0:0:0"/>
                                                    <ColorAnimation
                                                                            Storyboard.TargetName="stroke"
                                                                            Storyboard.TargetProperty="Color"
                                                                            From="Blue" To="Transparent" Duration="0:0:0.2" BeginTime="0:0:1.8"/>
                                                    <ColorAnimation
                                                                            Storyboard.TargetName="external"
                                                                            Storyboard.TargetProperty="Color"
                                                                            From="Blue" To="Transparent" Duration="0:0:0.2" BeginTime="0:0:1.8"/>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Ellipse.Triggers>
                                </Ellipse>
                            </Grid>
